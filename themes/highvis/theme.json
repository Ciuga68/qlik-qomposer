		"#ffd300",
		"#ffff00",
		"#aaff00",
		"#00ff00",
		"#00ffff",
		"#0551ff",
		"#4207ff",
		"#a603ff",
		"#ff0090",
		"#eeeeee",
		"#888888",
		"#222222"
	],
