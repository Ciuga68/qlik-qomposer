{
	"_themeName": "Qlikview High Visibility",
	"_owner": "Tobias Åström (tsm)",
	"backgroundColor": "#333",
	"fontFamily": "QlikView Sans",
	"fontSize": "15px",
	"fontWeight": "normal",
	"color": "#eee",
	"axis": {
		"title": {
			"fontSize": {
				"default": "15px",
				"xsmall": "15px",
				"small": "15px",
				"medium": "15px",
				"full": "18px"
			},
			"color": {
				"default": "#eee",
				"active": "#eee",
				"inactive": "#999",
				"locked": "#6abbdd",
				"disabled": "#999"
			}
		},
		"label": {
			"fontSize": {
				"default": "12px",
				"xsmall": "11px",
				"small": "12px",
				"medium": "14px",
				"full": "16px"
			},
			"discrete": {
				"fontSize": {
					"default": "12px",
					"xsmall": "11px",
					"small": "12px",
					"medium": "14px",
					"full": "16px"
				}
			},
			"margin": 4
		},
		"tick": {
			"color": "#FCFCB1",
			"length": 8
		},
		"line": {
			"color": "#FCFDC4"
		},
		"grid": {
			"major": {
				"color":"#FCFDC4",
				"width": 1
			},
			"minor": {
				"color":"#FEFEE6",
				"width": 1
			},
			"zero": {
				"color":"#F7F73B",
				"width": 1
			}
		}
	},
	"legend": {
		"title": {
			"color": "#fff",
			"fontSize": "12px"
		},
		"label": {
			"color": "#eee",
			"fontSize": "14px"
		}
	},
	"item": {
		"name": {
			"color": "#aaa",
			"fontFamily": "QlikView Sans",
			"fontSize": "15px",
			"fontWeight": "normal"
		},
		"value": {
			"color": "#aaa",
			"fontFamily": "QlikView Sans",
			"fontSize": "14px"
		}
	},
	"gauge": {
		"value": {
			"color": "#eee",
			"fontSize": "42px",
			"fontWeight": "bold",
			"minFontSize": 25,
			"maxFontSize": 72
		},
		"area": {
			"color": "#eeeeee"
		},
		"pointer": {
			"color": "#eeeeee"
		}
	},
	"treemap": {
		"branch": {
			"fill": "rgba(255,255,255,0.05)",
			"label": {
				"color": "rgba(222,222,222,1)",
				"fontSize": "16px",
				"fontWeight": "bold"
			}
		},
		"leaf": {
			"border": {
				"width": {
					"default": 2,
					"selected": 3
				},
				"color": {
					"default": "#222",
					"selected": "#eee"
				}
			},
			"label": {
				"fontSize": "14px",
				"fontWeight": "bold",
				"color": {
					"default": "#666",
					"light": "#eee",
					"dark": "#666"
				}
			}
		},
		"overlay": {
			"label": {
				"color": {
					"default": "rgba(33,33,33,0.4)",
					"light": "rgba(222,222,222,0.4)",
					"dark": "rgba(33,33,33,0.4)"
				},
				"fontWeight": "bold"
			}
		}
	},
	"scatterPlot": {
		"value": {
			"color": "#eee",
			"fontSize": "12px",
			"background": "rgba(51,51,51,0.9)"
		},
		"shape": {
			"border": {
				"width": {
					"default": 1,
					"selected": 2
				},
				"color": "rgba(238,238,238,0.7)"
			},
			"outOfBounds": {
				"opacity": 0.3
			}
		},
		"line": {
			"color": "#999",
			"width": 1
		}
	},
	"barChart": {
		"shape": {
			"border": {
				"color": {
					"default": "rgba(0,0,0,0.25)",
					"selected": "#eee"
				},
				"width": {
					"default": 0,
					"selected": 3
				}
			}
		},
		"value": {
			"color": {
				"default": "#666",
				"dark": "#666",
				"light": "#eee"
			},
			"fontSize": {
				"default": "14px",
				"full": "14px",
				"medium": "14px",
				"small": "14px",
				"xsmall": "14px",
				"spark": "14px"
			}
		}
	},
	"lineChart": {
		"fontSize": "13px",
		"color": "#aaa",
		"item": {
			"value": {
				"color": {
					"default": "#eee",
					"dark": "#666",
					"light": "#eee"
				},
				"fontSize": {
					"default": "12px",
					"full": "16px",
					"medium": "14px",
					"small": "12px",
					"xsmall": "10px",
					"spark": "10px"
				}
			},
			"shape": {
				"size": {
					"default": 10,
					"selected": 14
				}
			},
			"line": {
				"width": {
					"default": 2,
					"full": 3,
					"medium": 2,
					"small": 2,
					"xsmall": 1,
					"spark": 1
				}
			},
			"outOfRange": {
				"color": "#999999"
			}
		}
	},
	"pieChart": {
		"color": "#aaa",
		"axis": {
			"title": {
				"fontSize": "16px",
				"color": {
					"default": "#333",
					"active": "#333",
					"inactive": "#999"
				}
			},
			"label": {
				"fontSize": "14px"
			}
		},
		"item": {
			"name": {
				"fontWeight": "bold",
				"fontSize": {
					"default": "15px",
					"large": "18px",
					"medium": "16px",
					"small": "14px",
					"tiny": "12px"
				},
				"color": {
					"default": "#aaa",
					"dark": "#333",
					"light": "#eee"
				}
			},
			"value": {
				"color": {
					"default": "#666",
					"dark": "#333",
					"light": "#eee"
				},
				"fontSize": {
					"default": "14px",
					"large": "16px",
					"medium": "14px",
					"small": "12px",
					"tiny": "11px"
				}
			},
			"shape": {
				"border": {
					"width": {
						"default": 1,
						"selected": 2
					 }
				}
			},
			"line": {
				"width": 1,
				"color": "#aaa"
			}
		}
	},
	"palette": 
	[
		"#ff0000",
