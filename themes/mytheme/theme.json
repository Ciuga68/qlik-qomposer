{
	"_themeName": "Qlik Sense Default",
	"_variables": {
		"B35": "#595959",
		"B90": "#E6E6E6",
		"B20": "#333333",
		"titleColor": "@B20",
		"labelColorDark": "@B35",
		"labelColorLight": "@B90"
	},
	"backgroundColor": "#fff",
	"fontFamily": "QlikView Sans",
	"fontSize": "13px",
	"fontWeight": "normal",
	"color": "@labelColorDark",
	"axis": {
		"title": {
			"fontSize": {
				"default": "12px",
				"xsmall": "12px",
				"small": "12px",
				"medium": "12px",
				"full": "14px"
			},
			"color": {
				"default": "@titleColor",
				"active": "@titleColor",
				"inactive": "#999",
				"locked": "#6abbdd",
				"disabled": "#999"
			}
		},
		"label": {
			"fontSize": {
				"default": "12px",
				"xsmall": "10px",
				"small": "11px",
				"medium": "12px",
				"full": "14px"
			},
			"discrete": {
				"fontSize": {
					"default": "12px",
					"xsmall": "10px",
					"small": "11px",
					"medium": "12px",
					"full": "12px"
				}
			},
			"margin": 4
		},
		"tick": {
			"color": "#d6d6d6",
			"length": 8
		},
		"line": {
			"color": "#c3c3c3"
		},
		"grid": {
			"major": {
				"color":"#e0e0e0"
			},
			"minor": {
				"color":"#f3f3f3"
			},
			"zero": {
				"color":"#999"
			},
			"width": {
				"default": 1,
				"full": 2,
				"medium": 1,
				"small": 1,
				"xsmall": 1,
				"spark": 1
			}
		}
	},
	"legend": {
		"title": {
			"color": "@titleColor",
			"fontSize": "15px",
			"lineHeight": "1em"
		},
		"label": {
			"color": "@labelColorDark",
			"fontSize": "13px",
			"lineHeight": "1em"
		}
	},
	"item": {
		"name": {
			"color": "@labelColorDark",
			"fontSize": "13px"
		},
		"value": {
			"color": "@labelColorDark",
			"fontSize": "12px"
		}
	},
	"gauge": {
		"value": {
			"color": "@titleColor",
			"fontSize": "42px",
			"minFontSize": 12,
			"maxFontSize": 60
		},
		"area": {
			"backgroundColor": "#eeeeee"
		},
		"pointer": {
			"color": {
				"light": "@B90",
				"dark": "@B20"
			}
		}
	},
	"treemap": {
		"branch": {
			"fill": "rgba(0,0,0,0.05)",
			"label": {
				"color": "rgba(50,50,50,1)",
				"fontSize": "14px"
			}
		},
		"leaf": {
			"border": {
				"width": {
					"default": 1,
					"selected": 2
				},
				"color": {
					"default": "#eee",
					"selected": "#000000"
				}
			},
			"label": {
				"fontSize": "12px",
				"color": {
					"default": "@labelColorDark",
					"light": "@labelColorLight",
					"dark": "@labelColorDark"
				}
			}
		},
		"overlay": {
			"label": {
				"color": {
					"default": "rgba(33,33,33,0.4)",
					"light": "rgba(222,222,222,0.4)",
					"dark": "rgba(33,33,33,0.4)"
				},
				"fontWeight": "bold"
			}
		}
	},
	"scatterPlot": {
		"value": {
			"color": "@labelColorDark",
			"fontSize": "12px",
			"background": "rgba(255,255,255,0.9)"
		},
		"shape": {
			"border": {
				"width": {
					"default": 1,
					"selected": 2
				},
				"color": "rgba(0,0,0,0.7)"
			},
			"outOfBounds": {
				"opacity": 0.3
			}
		},
		"line": {
			"color": "#999",
			"width": 1
		}
	},
	"barChart": {
		"shape": {
			"border": {
				"color": {
					"default": "rgba(0,0,0,0.25)",
					"selected": "@titleColor"
				},
				"width": {
					"default": 0,
					"selected": 2
				}
			}
		},
		"value": {
			"color": {
				"default": "@labelColorDark",
				"dark": "@labelColorDark",
				"light": "@labelColorLight"
			},
			"fontSize": {
				"default": "11px",
				"full": "11px",
				"medium": "11px",
				"small": "11px",
				"xsmall": "11px",
				"spark": "11px"
			}
		}
	},
	"lineChart": {
		"fontSize": "13px",
		"color": "@labelColorDark",
		"item": {
			"value": {
				"color": {
					"default": "@labelColorDark",
					"dark": "@labelColorDark",
					"light": "@labelColorLight"
				},
				"fontSize": {
					"default": "12px",
					"full": "12px",
					"medium": "12px",
					"small": "12px",
					"xsmall": "12px",
					"spark": "12px"
				}
			},
			"shape": {
				"size": {
					"default": 6,
					"selected": 10
				}
			},
			"line": {
				"width": {
					"default": 1,
					"full": 2,
					"medium": 1,
					"small": 1,
					"xsmall": 1,
					"spark": 1
				}
			},
			"outOfRange": {
				"color": "#999999"
			}
		}
	},
	"pieChart": {
		"fontSize": "13px",
		"color": "@labelColorDark",
		"axis": {
			"title": {
				"fontSize": "14px",
				"color": {
					"default": "@titleColor",
					"active": "@titleColor",
					"inactive": "#999"
				}
			}
		},
		"item": {
			"name": {
				"fontSize": {
					"default": "13px",
					"large": "16px",
					"medium": "14px",
					"small": "12px",
					"tiny": "10px"
				},
				"color": {
					"default": "@labelColorDark",
					"dark": "@titleColor",
					"light": "@labelColorLight"
				}
			},
			"value": {
				"color": {
					"default": "@labelColorDark",
					"dark": "@titleColor",
					"light": "@labelColorLight"
				},
				"fontSize": {
					"default": "12px",
					"large": "14px",
					"medium": "12px",
					"small": "10px",
					"tiny": "9px"
				}
			},
			"shape": {
				"border": {
					"width": {
						"default": 1,
						"selected": 2
					 },
					 "color": {
						"default": "#eeeeee",
						"selected": "@titleColor"
					 }
				}
			},
			"line": {
				"width": 1,
				"color": "#999"
			}
		}
	},
	"palette": 
	[
		"#b0afae",
		"#7b7a78",
		"#545352",
		"#4477aa",
		"#7db8da",
		"#b6d7ea",
		"#46c646",
		"#f93f17",
		"#ffcf02",
		"#276e27",
		"#ffffff",
		"#000000"
